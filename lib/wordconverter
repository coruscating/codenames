#filein="wordlist_starwars"
#fileout="locations_starwars.js"

filein="wordlist2"
fileout="wordlist_2.js"

sort -u wordlist2 -o wordlist2

echo "wordlist_2 = [" > $fileout

while read -r line
do
    name=$line
    echo "{name: '$line'}," >> $fileout
done < $filein


sed -i '' '$s/,//' $fileout
echo "];" >> $fileout

